{
  "name": "apollo-graphql-api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && tscpaths -p tsconfig.json -s src -o dist",
    "dev": "sh ./scripts/start.sh",
    "start": "node  dist/index.js",
    "start:dev": "tsx watch src",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "pnpm run lint && pnpm run format",
    "fix": "pnpm run lint:fix && pnpm run format",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:import": "tsx seed/import.ts",
    "db:export": "tsx seed/export.ts"
  },
  "engines": {
    "node": ">=20.0"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@aws-sdk/client-bedrock-runtime": "^3.609.0",
    "@aws-sdk/client-lambda": "^3.609.0",
    "@aws-sdk/client-s3": "^3.609.0",
    "@aws-sdk/client-sts": "^3.609.0",
    "@aws-sdk/client-transcribe": "^3.609.0",
    "@aws-sdk/lib-storage": "^3.609.0",
    "@aws-sdk/s3-request-presigner": "^3.609.0",
    "@clerk/clerk-sdk-node": "^5.0.14",
    "@graphql-tools/schema": "^10.0.4",
    "@graphql-tools/utils": "^10.3.1",
    "@langchain/community": "^0.2.17",
    "@langchain/core": "^0.2.14",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^1.25.1",
    "@opentelemetry/id-generator-aws-xray": "^1.2.2",
    "@opentelemetry/instrumentation": "^0.52.1",
    "@opentelemetry/instrumentation-aws-sdk": "^0.43.0",
    "@opentelemetry/instrumentation-express": "^0.41.0",
    "@opentelemetry/instrumentation-graphql": "^0.42.0",
    "@opentelemetry/instrumentation-http": "^0.52.1",
    "@opentelemetry/propagator-aws-xray": "^1.25.1",
    "@opentelemetry/resources": "^1.25.1",
    "@opentelemetry/sdk-trace-base": "^1.25.1",
    "@opentelemetry/sdk-trace-node": "^1.25.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "elevenlabs": "^0.8.2",
    "express": "^4.19.2",
    "globals": "^15.8.0",
    "graphql": "^16.9.0",
    "graphql-redis-subscriptions": "^2.6.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-ws": "^5.16.0",
    "mongoose": "^8.4.5",
    "pino": "^9.2.0",
    "tscpaths": "^0.0.9",
    "uuidv4": "^6.2.13",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "pino-pretty": "^11.2.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0"
  }
}
