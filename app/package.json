{
  "name": "apollo-graphql-api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && tscpaths -p tsconfig.json -s src -o dist",
    "predev": "pnpm run generate",
    "dev": "sh ./scripts/start.sh",
    "start": "node -r newrelic dist/index.js",
    "start:dev": "tsx watch src",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "pnpm run lint && pnpm run format",
    "fix": "pnpm run lint:fix && pnpm run format",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:import": "tsx seed/import.ts",
    "db:export": "tsx seed/export.ts",
    "generate": "graphql-codegen --config codegen.ts"
  },
  "engines": {
    "node": ">=20.0"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@aws-sdk/client-bedrock-runtime": "^3.614.0",
    "@aws-sdk/client-lambda": "^3.614.0",
    "@aws-sdk/client-s3": "^3.614.0",
    "@aws-sdk/client-sts": "^3.614.0",
    "@aws-sdk/client-transcribe": "^3.614.0",
    "@aws-sdk/lib-storage": "^3.614.0",
    "@aws-sdk/s3-request-presigner": "^3.614.0",
    "@clerk/clerk-sdk-node": "^5.0.15",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-tools/graphql-file-loader": "^8.0.1",
    "@graphql-tools/load": "^8.0.2",
    "@graphql-tools/schema": "^10.0.4",
    "@graphql-tools/utils": "^10.3.2",
    "@langchain/community": "^0.2.18",
    "@langchain/core": "^0.2.15",
    "@newrelic/apollo-server-plugin": "^5.2.0",
    "@newrelic/pino-enricher": "^1.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "elevenlabs": "^0.9.1",
    "express": "^4.19.2",
    "globals": "^15.8.0",
    "graphql": "^16.9.0",
    "graphql-redis-subscriptions": "^2.6.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.2",
    "graphql-ws": "^5.16.0",
    "mongoose": "^8.5.1",
    "newrelic": "^11.23.1",
    "pino": "^9.3.1",
    "tscpaths": "^0.0.9",
    "uuidv4": "^6.2.13",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.0.9",
    "@graphql-codegen/typescript-resolvers": "4.2.1",
    "@parcel/watcher": "^2.4.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "pino-pretty": "^11.2.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1"
  }
}
