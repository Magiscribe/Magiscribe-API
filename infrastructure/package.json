{
  "name": "serverless-cdktf",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "deploy:network": "cdktf deploy network",
    "deploy:lambdas": "cdktf deploy lambdas",
    "deploy:app": "cdktf deploy app",
    "deploy:client": "cdktf deploy client",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "pnpm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "pnpm i cdktf@next cdktf-cli@next",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "fix": "pnpm run lint:fix && pnpm run format"
  },
  "engines": {
    "node": ">=18.0"
  },
  "dependencies": {
