    "@types/node": "^20.12.10",
    "eslint": "^9.2.0",
    "globals": "^15.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
