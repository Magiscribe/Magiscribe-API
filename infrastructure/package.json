    "@aws-sdk/client-sfn": "^3.569.0",
    "@aws-sdk/client-sqs": "^3.569.0",
    "@cdktf/provider-aws": "19.17.0",
    "@cdktf/provider-random": "^11.0.1",
    "@langchain/anthropic": "^0.1.17",
    "@langchain/community": "^0.0.55",
    "aws-lambda": "^1.0.7",
    "aws-xray-sdk": "^3.6.0",
    "cdktf": "^0.20.7",
    "cdktf-cli": "^0.20.7",
    "constructs": "^10.3.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.20.2",
