{
  "name": "my-graphql-project",
  "version": "1.0.0",
  "description": "A simple GraphQL API with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" src/index.ts",
    "start": "node  dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "fix": "pnpm run lint:fix && pnpm run format"
  },
  "author": "Luc",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@aws-sdk/client-bedrock-runtime": "^3.564.0",
    "@aws-sdk/client-lambda": "^3.564.0",
    "dotenv": "^16.4.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.19.2",
    "globals": "^15.1.0",
    "graphql": "^16.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "eslint": "^9.1.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "packageManager": "pnpm@9.0.2+sha1.2a34aeb80bf8e8435815d14f620ec4ba0eff1604"
