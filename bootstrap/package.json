{
  "name": "bootstrap",
  "version": "1.0.0",
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "deploy": "cdktf deploy",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "pnpm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "pnpm i cdktf@next cdktf-cli@next",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "pnpm run lint && pnpm run format",
    "fix": "pnpm run lint:fix && pnpm run format"
  },
  "engines": {
    "node": ">=20.0"
  },
  "dependencies": {
    "@cdktf/provider-aws": "19.31.0",
    "cdktf": "^0.20.8",
    "cdktf-cli": "^0.20.8",
    "constructs": "^10.3.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.23.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/aws-lambda": "^8.10.143",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.4.1",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0"
  }
}
